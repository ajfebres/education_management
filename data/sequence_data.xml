<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequence for Enrollment References -->
        <record id="seq_education_enrollment" model="ir.sequence">
            <field name="name">Education Enrollment Reference</field>
            <field name="code">education.enrollment</field>
            <field name="prefix">ENR/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
            <field name="active">True</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        

    </data>

</odoo>